{
  "productName": "fr-script",
  "version": "2.1.1",
  "identifier": "site.csfr",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon32x32.png",
      "icons/icon64x64.png",
      "icons/icon128x128.png",
      "icons/icon256x256.png",
      "icons/icon.ico",
      "icons/icon.png"
    ],
    "resources": [
      "resources/*"
    ]
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "decorations": false,
        "title": "风染脚本",
        "transparent": true,
        "width": 980,
        "height": 600,
        "minWidth": 700,
        "dragDropEnabled": false,
        "visible": false,
        "shadow": false
      },
      {
        "width": 400,
        "height": 200,
        "decorations": false,
        "transparent": true,
        "center": true,
        "url": "splashscreen.html",
        "label": "splashscreen",
        "focus": true,
        "shadow": false
      }
    ]
  },
  "plugins": {}
}